  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/node": "^16.6.2",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "typescript": "^4.3.5"
  }
